{
  "name": "@vertaai/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "prisma generate && (tsc --skipLibCheck || exit 0)",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "migrate:contract-packs": "tsx src/scripts/migrate-contract-packs.ts",
    "migrate:drift-plans": "tsx src/scripts/migrate-drift-plans.ts",
    "migrate:gatekeeper-to-yaml": "tsx src/scripts/migrate-legacy-gatekeeper-to-yaml.ts",
    "setup:e2e-workspace": "tsx src/scripts/create-test-workspace-and-pack.ts",
    "rollback:policy-packs": "tsx src/scripts/rollback-policy-packs.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "@notionhq/client": "^5.8.0",
    "@prisma/client": "^5.14.0",
    "@slack/bolt": "^3.18.0",
    "@slack/web-api": "^7.13.0",
    "@upstash/qstash": "^2.8.4",
    "@upstash/redis": "^1.36.1",
    "@vertaai/shared": "workspace:*",
    "bullmq": "^5.7.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "diff": "^5.2.2",
    "express": "^4.19.2",
    "ioredis": "^5.4.1",
    "js-yaml": "^4.1.1",
    "minimatch": "^10.2.1",
    "octokit": "^4.1.4",
    "re2": "^1.23.3",
    "semver": "^7.7.4",
    "uuid": "^13.0.0",
    "yaml": "^2.8.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.17",
    "@types/diff": "^5.2.3",
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.12.0",
    "@types/semver": "^7.7.1",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^11.0.0",
    "prisma": "^5.14.0",
    "supertest": "^7.2.2",
    "tsx": "^4.10.0",
    "typescript": "^5.4.0",
    "vitest": "^4.0.18"
  }
}
