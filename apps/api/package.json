    "@upstash/qstash": "^2.8.4",
    "@upstash/redis": "^1.36.1",
    "@vertaai/shared": "workspace:*",
    "bullmq": "^5.7.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "diff": "^5.2.2",
    "express": "^4.19.2",
    "ioredis": "^5.4.1",
    "js-yaml": "^4.1.1",
    "octokit": "^4.1.4",
